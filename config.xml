<?xml version='1.0' encoding='utf-8'?>
<widget id="com.mirrel.keygent" version="1.2.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
   <name>Keygen</name>
    <description>App Keygen</description>
    <author email="dev@cordova.apache.org" href="https://cordova.apache.org">
        Apache Cordova Team
    </author>
    <access origin="*" /> 
    <content src="index.html" /> 
    <allow-navigation href="*" />
    <allow-intent href="*" /> 
    <preference name="Scheme" value="https" />
    <preference name="Scheme" value="http" />
    <preference name="usesCleartextTraffic" value="true" /> 
    <preference name="AndroidPersistentFileLocation" value="Compatibility" />
    <platform name="android">
        <allow-intent href="market:*" /> 
        <icon density="xhdpi" height="96" src="res/icon/android/icon-96-xhdpi.png" width="96" /> 
    </platform>
</widget>
